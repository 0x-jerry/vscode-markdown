{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "outDir": "out",
    "lib": ["es2019", "es2020.bigint", "es2020.promise", "es2020.string"],
    "forceConsistentCasingInFileNames": true,
    "newLine": "lf",
    "sourceMap": true,
    "rootDir": ".",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", ".vscode-test"]
}
